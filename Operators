# Comparison Operator
print("Comparison Operator")
if a>b:
    print("A is Greater than B")
else:
    print("B is greater than A")

# Identity Operator
x = 14
y = x
print("Identity Operator")
print(x is y)
print(x is not y)

# Membership Operator
print(" Membership Operator")
p = " Apple "
print(" apple " in p)
